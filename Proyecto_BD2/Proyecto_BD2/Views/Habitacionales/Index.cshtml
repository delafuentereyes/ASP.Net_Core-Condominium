@{
	ViewData["Title"] = "Proyectos Habitacionales";
}

@Html.Partial("userAccess")

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script>
<div class="text-center">
	<div class="container">
		<form asp-controller="Habitacionales" asp-action="Index" method="post" enctype="multipart/form-data">
				@foreach (var @item in @ViewBag.Habitacional)
				{
					<option value="@item.Nombre_Habitacional"> @item.Nombre_Habitacional </option>
				}
		
		</form>
	</div>
</div>